{
  "profiles": {
    "Launch Client": {
      "commandName": "Executable",
      "executablePath": "%VINTAGE_STORY%\\$(RunGameVersion)\\Vintagestory.exe",
      "commandLineArgs": "--addModPath $(ProjectDir)mods --addOrigin $(ProjectDir)assets --tracelog",
      "workingDirectory": "%VINTAGE_STORY%\\$(RunGameVersion)"
    },
    "Launch Client with Test World": {
      "commandName": "Executable",
      "executablePath": "%VINTAGE_STORY%\\$(RunGameVersion)\\Vintagestory.exe",
      "commandLineArgs": "--addModPath $(ProjectDir)mods --addOrigin $(ProjectDir)assets --tracelog --playStyle \"preset-surviveandbuild\" --openWorld \"$(ProjectName) Test World\"",
      "workingDirectory": "%VINTAGE_STORY%\\$(RunGameVersion)"
    },
    "Launch Server": {
      "commandName": "Executable",
      "executablePath": "%VINTAGE_STORY%\\$(RunGameVersion)\\VintagestoryServer.exe",
      "commandLineArgs": "--addModPath $(ProjectDir)mods --addOrigin $(ProjectDir)assets --tracelog",
      "workingDirectory": "%VINTAGE_STORY%\\$(RunGameVersion)"
    },
    "Launch Client 2": {
      "commandName": "Executable",
      "executablePath": "%VINTAGE_STORY%\\$(RunGameVersion)\\Vintagestory.exe",
      "commandLineArgs": "--addModPath $(ProjectDir)mods --addOrigin $(ProjectDir)assets --tracelog --dataPath %VINTAGE_STORY_DATA2%",
      "workingDirectory": "%VINTAGE_STORY%\\$(RunGameVersion)"
    }
  }
}
